<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>cargaCombos</servlet-name>
        <servlet-class>Servlets.cargaCombos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>buscaArticulos</servlet-name>
        <servlet-class>Servlets.buscaArticulos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>creaArticulo</servlet-name>
        <servlet-class>creaArticulo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesion</servlet-name>
        <servlet-class>CerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserPanel</servlet-name>
        <servlet-class>UserPanel</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerAportaciones</servlet-name>
        <servlet-class>userPanel.VerAportaciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cargaAleatorioyRecientes</servlet-name>
        <servlet-class>Servlets.cargaAleatorioyRecientes</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cargaCombos</servlet-name>
        <url-pattern>/cargaCombos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>buscaArticulos</servlet-name>
        <url-pattern>/buscaArticulos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>creaArticulo</servlet-name>
        <url-pattern>/creaArticulo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesion</servlet-name>
        <url-pattern>/CerrarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserPanel</servlet-name>
        <url-pattern>/UserPanel</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerAportaciones</servlet-name>
        <url-pattern>/VerAportaciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cargaAleatorioyRecientes</servlet-name>
        <url-pattern>/cargaAleatorioyRecientes</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
